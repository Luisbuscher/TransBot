{
  "name": "transbot",
  "version": "2.0",
  "main": "main.js",
  "author": "",
  "description": "",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "package-win": "electron-packager . BotElectron --platform=win32 --arch=x64 --out=dist/ --overwrite"
  },
  "keywords": [],
  "dependencies": {
    "dotenv": "^16.4.7",
    "puppeteer-core": "^24.6.0"
  },
  "devDependencies": {
    "electron": "^35.1.3",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "luisbuscher",
    "productName": "Transbot",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "extraResources": [
      {
        "from": "chrome-win64/",
        "to": "chrome-win64/"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}